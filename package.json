{
  "name": "git-timecard",
  "description": "Single-user git based timecard.",
  "version": "0.4.1",
  "bin": {
    "gtc": "cli.js"
  },
  "scripts": {
    "test": "healthier && node cli",
    "posttest": "npm run format",
    "prepare": "husky install",
    "format": "prettier --loglevel warn --write \"**/*.{js,css,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/millette/git-timecard.git"
  },
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 12.14.1"
  },
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5"
  },
  "dependencies": {
    "isomorphic-git": "^1.7.3",
    "meow": "^7.0.1",
    "update-notifier": "^4.1.0"
  },
  "devDependencies": {
    "healthier": "^4.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "mrm": "^3.0.1",
    "prettier": "^2.3.0"
  }
}
