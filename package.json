{
  "name": "git-timecard",
  "description": "Single-user git based timecard.",
  "version": "0.5.2",
  "bin": {
    "gtc": "cli.mjs"
  },
  "scripts": {
    "test": "healthier && node cli.mjs",
    "posttest": "npm run format",
    "prepare": "husky install",
    "format": "prettier --loglevel warn --write \"**/*.{json,js,mjs,css,md}\"",
    "fix": "verdaccio-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/millette/git-timecard.git"
  },
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{json,js,mjs,css,md}": "prettier --write"
  },
  "dependencies": {
    "isomorphic-git": "^1.8.3",
    "meow": "^10.0.1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "healthier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.0.0",
    "mrm": "^3.0.10",
    "prettier": "^2.3.1",
    "verdaccio-fix": "^0.3.4"
  }
}
