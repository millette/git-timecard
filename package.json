{
  "name": "git-timecard",
  "description": "Single-user git based timecard.",
  "version": "0.2.1",
  "bin": {
    "gtc": "cli.js"
  },
  "scripts": {
    "test": "healthier && node cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/millette/git-timecard.git"
  },
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 12.14.1"
  },
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "dependencies": {
    "isomorphic-git": "^0.75.1",
    "meow": "^6.0.0",
    "update-notifier": "^4.0.0"
  },
  "devDependencies": {
    "healthier": "^3.2.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1"
  }
}
