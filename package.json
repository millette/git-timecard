{
  "name": "git-timecard",
  "description": "Single-user git based timecard.",
  "version": "0.6.0",
  "type": "module",
  "bin": {
    "gtc": "cli.js"
  },
  "scripts": {
    "test": "healthier && node cli.js",
    "posttest": "npm run format",
    "prepare": "husky install",
    "format": "prettier --loglevel warn --write \"**/*.{json,js,css,md}\"",
    "fix": "verdaccio-fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/millette/git-timecard.git"
  },
  "author": {
    "name": "Robin Millette",
    "email": "robin@millette.info",
    "url": "http://robin.millette.info"
  },
  "engines": {
    "node": ">= 14.17.0"
  },
  "license": "AGPL-3.0",
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{json,js,css,md}": "prettier --write"
  },
  "dependencies": {
    "isomorphic-git": "^1.18.2",
    "meow": "^10.1.2",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "healthier": "^5.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "mrm": "^4.0.0",
    "prettier": "^2.7.0",
    "verdaccio-fix": "^0.3.4"
  }
}
